<view>
  <view>
    <van-checkbox-group value="{{ result }}" bind:change="onChangeCheck">
      <van-checkbox name="{{item.studentid}}" wx:for="{{list}}">{{item.studentname}}</van-checkbox>
    </van-checkbox-group>
  </view>
  <view class = "buttonCon">
    <van-button round id="1" type="info"  bindtap="changetomessage">
      群发消息
    </van-button>
  </view>
  <van-tabbar active="{{ active }}" bind:change="onChange">
    <van-tabbar-item icon="home-o" info = "{{jwcMsgCount}}">教务处消息</van-tabbar-item>
    <van-tabbar-item icon="search">历史消息列表</van-tabbar-item>
    <van-tabbar-item icon="friends-o">群发消息</van-tabbar-item>
    <van-tabbar-item icon="setting-o">设置</van-tabbar-item>
  </van-tabbar>
</view>